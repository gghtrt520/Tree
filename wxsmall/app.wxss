@import "colorui/main.wxss";
@import "colorui/icon.wxss";
.bg-header-color{
  background: linear-gradient(45deg,green,rgb(0, 102, 255));
  background: -webkit-linear-gradient(45deg,green,rgb(0, 102, 255));
  background: -o-linear-gradient(45deg,green,rgb(0, 102, 255));
  background: -moz-linear-gradient(45deg,green,rgb(0, 102, 255));
  color:#fff;
}
.scrollPage{
  height: calc(100vh - 64px);
}
.nodata{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 40rpx;
}
.nodata > .cuIcon-warn{
  font-size: 42px;
}